@model notienendqver.Models.Material

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar Material</h1>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NombMaterial)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombMaterial)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DescrpMaterial)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DescrpMaterial)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PrecioMaterial)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PrecioMaterial)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaCreacion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaCreacion)
        </dd>
        <dt class = "col-sm-2">
